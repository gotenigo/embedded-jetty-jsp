<body>
  <h1>Create EMS config</h1>
  <form action="/createConfig" method="post">
  </form>
  <div id="res" class="alert"></div>
  <script type="text/javascript" src="deps/jquery.min.js"></script>
  <script type="text/javascript" src="deps/underscore.js"></script>
  <script type="text/javascript" src="deps/opt/jsv.js"></script>
  <script type="text/javascript" src="lib/jsonform.js"></script>
  <script type="text/javascript">
    $('form').jsonForm({
      schema: {
 "properties": {

   "configuration": {
     "type": "object",
     "properties": {
       "ems_file_connector": {
         "type": "object",
         "properties": {
           "enabled": {
             "type": "boolean",
             "default": false
           },
           "provider_folder_setup": {
             "type": "array",
             "items" : {
               "type" : "object",
               "properties": {
                 "folder": {
                   "type": "string"
                 },
                 "extension": {
                   "type": "string"
                 },
                 "archive": {
                   "type": "string"
                 },
                 "signature": {
                   "type": "string"
                 },
                 "error": {
                   "type": "string"
                 },
                 "retries": {
                   "type": "string"
                 },
                 "local": {
                   "type": "object",
                   "properties": {
                     "business_app": {
                       "type": "string"
                     },
                     "profile_name": {
                       "type": "string"
                     },
                     "shared_key": {
                       "type": "string"
                     }
                   }
                 },
                 "remote": {
                   "type": "object",
                   "properties": {
                     "p2p_url": {
                       "type": "string"
                     },
                     "business_app": {
                       "type": "string"
                     },
                     "profile_name": {
                       "type": "string"
                     },
                     "shared_key": {
                       "type": "string"
                     },
                     "cert_alias": {
                       "type": "string"
                     }
                   }
                 }
               }
             }
           },
           "receiver_folder_setup": {
             "type": "array",
             "items" : {
               "type" : "object",

               "properties": {
                 "folder": {
                   "type": "string"
                 },
                 "signature": {
                   "type": "string"
                 },
                 "signature_verification": {
                   "type": "string"
                 },
                 "local": {
                   "type": "object",
                   "properties": {
                     "business_app": {
                       "type": "string"
                     },
                     "profile_name": {
                       "type": "string"
                     }
                   }
                 }
               }
             }
           }
         }
       }
     }
   }


  }

      }
